<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_horizontal"
    android:background="@color/color_F0F2F2"
    android:orientation="vertical">
    <include layout="@layout/base_title" />
    <ImageView
        android:layout_width="@dimen/px240_dp"
        android:layout_height="@dimen/px240_dp"
        android:layout_marginTop="@dimen/px180_dp"
        android:background="@drawable/f1_ic_opt_ok"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/px60_dp"
        android:text="提现成功"
        android:textSize="@dimen/px42_sp"
        android:textColor="@color/color_838383"/>

    <Button
        android:id="@+id/btn_success"
        android:layout_width="match_parent"
        android:layout_height="@dimen/px114_dp"
        android:layout_marginTop="@dimen/px120_dp"
        android:layout_marginLeft="@dimen/px45_dp"
        android:layout_marginRight="@dimen/px45_dp"
        android:background="@drawable/f1_btn_phoneverify_selector"
        android:text="完成"
        android:textColor="@color/white"
        android:textSize="@dimen/px48_sp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/px60_dp"
        android:text="提现将在24小时内到账"
        android:textSize="@dimen/px42_sp"
        android:textColor="@color/color_D6D6D6"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/px30_dp"
            android:text="@string/delay_contact_qq"
            android:textSize="@dimen/px42_sp"
            android:textColor="@color/color_D6D6D6"/>

        <TextView
            android:id="@+id/tv_service_qq"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/px30_dp"
            android:text=""
            android:textSize="@dimen/px42_sp"
            android:textColor="@color/color_D6D6D6"/>
    </LinearLayout>
</LinearLayout>